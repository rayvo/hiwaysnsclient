<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:src="@drawable/btn_discharge_1"
		android:layout_height="wrap_content" android:id="@+id/imageView1"
		android:layout_width="wrap_content"></ImageView>
	<ImageView android:src="@drawable/btn_discharge"
		android:layout_height="wrap_content" android:id="@+id/imageView1"
		android:layout_width="wrap_content"></ImageView>
	<ImageView android:src="@drawable/btn_unplugged"
		android:layout_height="wrap_content" android:id="@+id/imageView1"
		android:layout_width="wrap_content"></ImageView>
	<ImageView android:id="@+id/imageButton1"
		android:layout_height="wrap_content" android:src="@drawable/btn_map_confirm"
		android:layout_width="wrap_content"></ImageView>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<LinearLayout android:orientation="vertical"
			android:layout_width="wrap_content" android:layout_height="wrap_content">
			<TextView android:layout_width="fill_parent"
				android:layout_height="125px" />
			<ImageView android:src="@drawable/btn_bps"
				android:layout_height="wrap_content" android:id="@+id/imageView1"
				android:layout_width="wrap_content"></ImageView>
			<ImageView android:src="@drawable/btn_bps_3"
				android:layout_height="wrap_content" android:id="@+id/imageView1"
				android:layout_width="wrap_content"></ImageView>
		</LinearLayout>
		<ImageView android:src="@drawable/btn_battery_10"
			android:layout_height="wrap_content" android:id="@+id/imageView1"
			android:layout_width="wrap_content"></ImageView>
		<LinearLayout android:orientation="vertical"
			android:layout_width="wrap_content" android:layout_height="wrap_content">
			<TextView android:layout_width="fill_parent"
				android:layout_height="125px" />
			<ImageView android:src="@drawable/btn_aps"
				android:layout_height="wrap_content" android:id="@+id/imageView1"
				android:layout_width="wrap_content"></ImageView>
			<ImageView android:src="@drawable/btn_aps_7"
				android:layout_height="wrap_content" android:id="@+id/imageView1"
				android:layout_width="wrap_content"></ImageView>
		</LinearLayout>
	</LinearLayout>

</LinearLayout>